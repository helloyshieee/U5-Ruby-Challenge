<h1>Products</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
    </tr>
  </thead>
  <tbody>
   <% @products.each do |product| %>
  <tr>
    <td><%= link_to product.name, product_path(product) %></td>
    <td><%= product.category.name %></td>
  </tr>
  <% end %>
  </tbody>
</table>
